roup\n          legend='Выберите соус'\n          register={register}\n          name='sauce'\n          options={SAUCE}\n        />\n\n        <CheckboxGroup\n          legend='Добавьте сыр'\n          register={register}\n          name='cheese'\n          options={CHEESE}\n        />\n\n        <CheckboxGroup\n          legend='Добавьте овощи'\n          register={register}\n          name='vegetables'\n          options={VEGETABLES}\n        />\n\n        <CheckboxGroup\n          legend='Добавьте мясо'\n          register={register}\n          name='meat'\n          options={MEAT}\n        />\n\n        <button>Заказать за {totalPrice}руб.</button>\n      </form>\n    </>\n  );\n};\n","F:\\artem-pizza\\client\\src\\share\\calculateTotalPrice.ts",[],"F:\\artem-pizza\\client\\src\\pages\\pizza-configurator-page\\state-ingredients\\selectors.ts",[],"F:\\artem-pizza\\client\\src\\pages\\pizza-configurator-page\\state-pizza\\selectors.ts",[],"F:\\artem-pizza\\client\\src\\pages\\pizza-configurator-page\\state-pizza\\actions.ts",[],"F:\\artem-pizza\\client\\src\\pages\\orders-list-page\\state\\reducer.ts",[],"F:\\artem-pizza\\client\\src\\pages\\pizza-configurator-page\\components\\OrderPreview.tsx",[],"F:\\artem-pizza\\client\\src\\pages\\checkout-page\\components\\OrderPreview.tsx",["228"],"import React from 'react';\n// Helpers\nimport { calculateTotalPrice } from '../../../share/calculateTotalPrice';\n// Helpers\nimport { renderIngredients } from '../../../share/renderIngredient';\n// Types\nimport { Ingredient, PizzaConfiguration } from '../../../types';\n\ninterface CheckoutPreviewProps {\n  pizza: PizzaConfiguration;\n  ingredients: Ingredient[];\n}\n\nexport const OrderPreview: React.FC<CheckoutPreviewProps> = ({\n  pizza,\n  ingredients,\n}) => {\n  const { size, dough, sauce, cheese, vegetables, meat } = pizza;\n\n  const CHEESE = ingredients.filter(i => i.category === 'cheese');\n  const VEGETABLES = ingredients.filter(i => i.category === 'vegetables');\n 